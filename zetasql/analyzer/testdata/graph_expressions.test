SELECT
  aml_1.a_5 AS same_node_a_b,
  aml_1.a_6 AS same_node_a,
  aml_1.a_7 AS same_edge
FROM
  GRAPH_TABLE(
    aml
    MATCH
      (a_2
      IS Person)
      -[a_3
      IS (% | ! %)]->
      (a_4
      IS Account)
    COLUMNS(
      SAME(a_2, a_4, a_2, a_4) AS a_5,
      SAME(a_2, a_2, a_2) AS a_6,
      SAME(a_3, a_3) AS a_7
    )
  ) AS aml_1;

