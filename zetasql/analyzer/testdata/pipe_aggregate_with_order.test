      +-OrderByItem
      | +-column_ref=
      |   +-ColumnRef(type=INT64, column=$groupby.$groupbycol3#6)
      +-OrderByItem
        +-column_ref=
          +-ColumnRef(type=INT64, column=$groupby.$groupbycol4#7)
