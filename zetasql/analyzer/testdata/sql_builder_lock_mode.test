# FOR UPDATE is not propagated to expression subquery in UNNEST even though its
# in the FROM clause.
select
  *
from
  KeyValue,
  unnest(ARRAY(select key from TestTable where KeyValue.key = TestTable.key))
for update
--
[SQLBUILDER_TARGET_SYNTAX_MODE standard]
[SQLBUILDER_OUTPUT]
SELECT
  keyvalue_3.a_1 AS Key,
  keyvalue_3.a_2 AS Value,
  a_5 AS a_5
FROM
  (
    SELECT
      KeyValue.Key AS a_1,
      KeyValue.Value AS a_2
    FROM
      KeyValue
    FOR UPDATE
  ) AS keyvalue_3
  JOIN
  UNNEST(ARRAY(
    SELECT
      TestTable.key AS a_4
    FROM
      TestTable
    WHERE
      (keyvalue_3.a_1) = CAST(TestTable.key AS INT64)
  )) AS a_5;

[SQLBUILDER_TARGET_SYNTAX_MODE pipe]
[SQLBUILDER_OUTPUT]
FROM
  KeyValue
FOR UPDATE
|> SELECT
     KeyValue.Key AS a_1,
     KeyValue.Value AS a_2
|> AS keyvalue_3
|> JOIN
   UNNEST(ARRAY(
     FROM
       TestTable
     |> WHERE
          (keyvalue_3.a_1) = CAST(TestTable.key AS INT64)
     |> SELECT
          TestTable.key AS a_4
   )) AS a_5
|> SELECT
     keyvalue_3.a_1 AS Key,
     keyvalue_3.a_2 AS Value,
     a_5 AS a_5;
