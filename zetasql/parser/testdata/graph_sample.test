[default reserve_graph_table]
[default language_features=NONE,+SQL_GRAPH]

select gt.* from
graph_table(aml
  match (n)
  let age = DATE_DIFF(CURRENT_DATE, n.birthday, YEAR)
  TABLESAMPLE 
    {{|bernoulli}} (
      5 percent
      {{|partition by (element_id(n))}}
    )
    {{|with weight as w}}
    {{|repeatable(1234)}}
  return n.name, 1 AS number
) gt

